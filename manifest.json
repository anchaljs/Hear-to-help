{
   "name": "Hear the website",
    "description": "An extension that reads your website",
    "version": "1.0",
    "manifest_version":3,
    
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js":["Content.js"],
            "html": ["model.html"]
        }
    ],
    
    "icons": {
          "16": "/image/picker_16.png",
          "32": "/image/picker_32.png",
          "48": "/image/picker_48.png"
    }

    
}